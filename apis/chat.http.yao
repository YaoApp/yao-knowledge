{
  "name": "OpenAPI会话",
  "version": "1.0.3",
  "description": "OpenAPI会话",
  "guard": "-",
  "paths": [
    {
      "path": "/stream",
      "method": "GET",
      "process": "scripts.qna.AnswerStream",
      "in": ["$query.question", "$query.user", "$query.sid"],
      "out": { "status": 200, "type": "application/json", "stream": true }
    }
  ]
}
