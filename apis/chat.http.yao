{
  "name": "OpenAPI会话",
  "version": "1.0.2",
  "description": "OpenAPI会话",
  "guard": "-",
  "paths": [
    {
      "path": "/stream",
      "method": "POST",
      "process": "scripts.qna.Stream",
      "in": ["$payload.question", "$payload.user", "$payload.sid"],
      "out": { "status": 200, "type": "application/json", "stream": true }
    }
  ]
}
